{
  "name": "svelte-dependency-viz",
  "description": "A direct dependency visualizer for Svelte",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.27",
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.9.8",
    "tslib": "^2.0.0",
    "typescript": "^4.4.3",
    "vite": "^2.6.7"
  },
  "repository": "https://github.com/MicLeey/svelte-dependency-viz",
  "homepage": "https://github.com/MicLeey/svelte-dependency-viz",
  "bugs": {
    "url": "https://github.com/MicLeey/svelte-dependency-viz/issues"
  },
  "type": "module",
  "keywords": [
    "svelte",
    "typescript",
    "visualization",
    "graph",
    "dependeny-graph",
    "progression-map"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs"
}
