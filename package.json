{
  "name": "svelte-dependency-viz",
  "description": "A direct dependency visualizer for Svelte",
  "version": "1.1.7",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit package",
    "preview": "svelte-kit preview",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.46.3",
    "svelte-check": "^2.4.1",
    "svelte-preprocess": "^4.10.2",
    "svelte2tsx": "^0.5.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MicLeey/svelte-dependency-viz"
  },
  "homepage": "https://github.com/MicLeey/svelte-dependency-viz",
  "bugs": {
    "url": "https://github.com/MicLeey/svelte-dependency-viz/issues"
  },
  "type": "module",
  "keywords": [
    "svelte",
    "typescript",
    "visualization",
    "graph",
    "dependeny-graph",
    "progression-map"
  ],
  "svelte": "index.js"
}
